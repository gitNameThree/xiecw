<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhrx.dao.UserDao">

    <select id="findUserByUserId" resultType="com.zhrx.entity.User">
        select * from zhrx_user where user_id = #{id}
